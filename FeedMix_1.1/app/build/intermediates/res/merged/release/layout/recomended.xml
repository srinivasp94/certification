<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="250dp"
        android:layout_height="400dp"
        android:layout_gravity="center"
        android:background="@drawable/alert_style2">
        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/alert_style1">
            <TextView
                android:id="@+id/titleTV"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:textColor="#fff"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
           <TextView
               android:id="@+id/protienTV"
               android:layout_gravity="center"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Protien:"
               android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/protienQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/protienLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/energyTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Energy:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/energyQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/energyLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/calsiumTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ca:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/calsiumQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/calsiumLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/avpTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="AvP:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/avpQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/avpLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/capTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ca:P Ratio:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/capQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/capLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/metTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Met:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/metQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/metLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/lysTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lys:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/lysQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/lysLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/fiberTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Fiber:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/fiberQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/fiberLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
            android:layout_marginLeft="10sp"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/sodiumTV"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sodium:"
                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/sodiumQnt"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>
            <TextView
                android:id="@+id/sodiumLVL"
                android:layout_gravity="center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:textColor="@color/protein"/>

        </LinearLayout>
        <LinearLayout
            android:layout_gravity="center"
           android:gravity="center"
            android:layout_weight="0.95"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <Button
                android:id="@+id/okBtn"
                android:layout_width="80sp"
                android:layout_height="30sp"
                android:text="OK"
                android:textColor="#fff"
                android:background="@drawable/button_border"/>
            </LinearLayout>

    </LinearLayout>

</LinearLayout>