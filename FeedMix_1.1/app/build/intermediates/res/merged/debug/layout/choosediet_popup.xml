<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <LinearLayout
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical"
        android:layout_width="300dp"

        android:layout_height="200dp">
        <LinearLayout
            android:orientation="vertical"
        android:layout_weight="1.3"
        android:layout_margin="10dp"
            android:layout_gravity="center"
            android:gravity="center"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
            android:background="@drawable/alert_style2">
            <LinearLayout
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_weight="4"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:background="@drawable/alert_style1">
                <TextView
                    android:id="@+id/title_TV"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:text="Maize"
                    android:textColor="#fff"
                    />

            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">
                        <TextView
                            android:id="@+id/protien_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textColor="#644635"
                            android:textSize="10sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Protein:"/>
                        <TextView
                            android:id="@+id/fat_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#644635"
                            android:text="Fat:"/>
                        <TextView
                            android:id="@+id/fiber_tv"
                            android:layout_weight="1"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Fiber:"/>
                        <TextView
                            android:id="@+id/energy_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Energy:"/>
                        <TextView
                            android:id="@+id/calsium_tv"
                            android:layout_weight="1"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Ca:"/>


                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">
                        <TextView
                            android:id="@+id/protien_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                           />
                        <TextView
                            android:id="@+id/fat_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#f09d4e"
                           />
                        <TextView
                            android:id="@+id/fiber_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                           />
                        <TextView
                            android:id="@+id/energy_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />
                        <TextView
                            android:id="@+id/calsium_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />


                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent">
                    <LinearLayout
                        android:layout_weight="0.89"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">
                        <TextView
                            android:id="@+id/avp_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="AvP:"/>
                        <TextView
                            android:id="@+id/sodium_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#644635"
                            android:text="Sodium:"/>
                        <TextView
                            android:id="@+id/methion_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textSize="10sp"
                            android:gravity="center"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Methionine:"/>
                        <TextView
                            android:id="@+id/lysin_tv"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:textSize="10sp"
                            android:gravity="center"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:text="Lysine:"/>
                        <TextView
                            android:id="@+id/quant_txt"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#644635"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent">
                        <TextView
                            android:id="@+id/avp_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />
                        <TextView
                            android:id="@+id/sodium_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:textColor="#f09d4e"

                            />
                        <TextView
                            android:id="@+id/methion_TV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />
                        <TextView
                            android:id="@+id/lysinTV"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />
                        <TextView
                            android:id="@+id/quant"
                            android:layout_weight="1"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="10sp"
                            android:textColor="#f09d4e"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            />


                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


    </LinearLayout>


    </LinearLayout>



</LinearLayout>